{
    "name": "daniel-jorg-schuppelius/php-api-toolkit",
    "description": "Reused codes for my api php sdks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Joerg Schuppelius",
            "email": "info@schuppelius.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2 || ^8.3",
        "psr/log": "^3.0@dev",
        "guzzlehttp/guzzle": "^7.9 || ^8.0",
        "dschuppelius/php-error-toolkit": "^0.1.0 || ^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "APIToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "APIToolkit\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3@dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
