{
    "name": "daniel-jorg-schuppelius/php-api-toolkit",
    "description": "Reused codes for my api php sdks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Joerg Schuppelius",
            "email": "info@schuppelius.org"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=8.1 <8.5",
        "guzzlehttp/guzzle": "^7.10",
        "dschuppelius/php-common-toolkit": "^0.9.9",
        "ramsey/uuid": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "APIToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3@dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
